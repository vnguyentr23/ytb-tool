# Environment Configuration
# Copy this file to .env and fill in your actual values

# GenAI API Keys
API_KEY_GEN=your_genaipro_api_key_here
API_KEY_AI33=your_ai33_api_key_here

# TTS Voice IDs
# GenAI Labs Voice ID (ElevenLabs format)
TTS_VOICE_ID_LABS=j210dv0vWm7fCknyQpbA

# GenAI Max Voice ID
TTS_VOICE_ID_MAX=your_max_voice_id_here

# Ngrok Configuration
# Start ngrok with: ngrok http 9999
# Then paste the URL here (e.g., https://xxxxx.ngrok-free.app)
NGROK_URL=

# Callback Server Configuration
CALLBACK_PORT=9999
CALLBACK_HOST=localhost

# Processing Configuration
CONCURRENCY=4
MAX_RETRIES=1

# Development Mode
NODE_ENV=production
